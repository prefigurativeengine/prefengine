{
    // See https://go.microsoft.com/fwlink/?LinkId=733558
    // for the documentation about the tasks.json format

    "version": "2.0.0",
    "tasks": [
        {
            "label": "win_32",
            "dependsOn": [
                "win_build_32",
                "cpy-search-lib"
            ],
            "group": "build",
            "dependsOrder": "sequence"
        },

        {
            "label": "win_build_32",
            "type": "shell",
            "command": ".\\win_build_cargo32.bat",
            "presentation":{
                "echo": true,
                "reveal": "always",
                "focus": false,
                "panel": "shared",
                "showReuseMessage": true,
                "clear": false
            },
            "options": {
                "cwd": "${workspaceFolder}"
            },
        },
        {
            "label": "cpy-cat-lib",
            "type": "shell",
            "command": "copy",
            "args": [
                "${workspaceRoot}/catalysis/target/32/debug/catalysis.*",
                "${workspaceRoot}/build",
            ],
            "presentation": {
            },
        },
        {
            "label": "cpy-search-lib",
            "type": "shell",
            "command": "copy",
            "args": [
                "C:/Users/HP/.rustup/toolchains/stable-i686-pc-windows-msvc/lib/rustlib/i686-pc-windows-msvc/lib/*.dll",
                "${workspaceRoot}/build/i686-pc-windows-msvc/debug/",
            ],
            "presentation": {
            },
        },
        {
            "label": "cpy-sandbox",
            "type": "shell",
            "command": "copy",
            "args": [
                "${workspaceRoot}/catalysis/target/32/debug/sandbox.*",
                "${workspaceRoot}/build",
            ],
            "presentation": {
            },
        },
    ]
}